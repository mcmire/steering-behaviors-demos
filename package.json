{
  "name": "steering-behaviors-demos",
  "version": "0.0.1",
  "private": true,
  "repository": "https://github.com/mcmire/steering-behaviors-demos",
  "description": "Demos of well-known steering behaviors pioneered by Craig Reynolds",
  "config": {
    "buildDir": "./docs",
    "stageWidth": 1920,
    "stageHeight": 1080
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --content-base $npm_package_config_buildDir --hot --inline --host 0.0.0.0",
    "webpack": "webpack -p --config ./webpack.config.js",
    "build": "NODE_ENV=production npm run webpack",
    "prewebpack": "npm run clean",
    "clean": "rm -rf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^16.3.4",
    "@types/lodash": "^4.14.80",
    "@types/pixi.js": "^4.5.6",
    "lodash": "^4.17.4",
    "pixi.js": "^4.5.6",
    "prettier": "^1.7.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "brfs": "^1.4.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.13.1",
    "postcss-loader": "^2.0.8",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "reactman": "^3.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "transform-loader": "^0.2.4",
    "ts-loader": "^3.0.5",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
